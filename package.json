    "vue-router": "^3.0.3",
    "vue-virtual-scroll-list": "^1.3.7",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/register": "^7.4.4",
    "@kazupon/vue-i18n-loader": "^0.3.0",
    "@vue/cli-plugin-babel": "^3.6.0",
    "@vue/cli-plugin-eslint": "^3.6.0",
